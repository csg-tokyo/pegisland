{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.js.map": true,
    "**/*.js": { "when": "$(basename).ts" },
    "**/**.js": { "when": "$(basename).tsx" },
    "**/*.d.ts": true,
    "**/*.d.ts.map": true
  },
  "cSpell.words": [
    "graphviz",
    "Katsumi",
    "Nonterminal",
    "Nonterminals",
    "Okuda",
    "Packrat",
    "Postorder",
    "subexp",
    "subname"
  ]
}
